    "path-browserify": "^1.0.1",
    "prettier": "2.5.1",
    "sass": "^1.26.11",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.0",
    "vite-plugin-pwa": "^0.17.0",
    "vite-svg-loader": "^5.0.0"
  },
  "resolutions": {
    "icon-gen": "3.0.0",
